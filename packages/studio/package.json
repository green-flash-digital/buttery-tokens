{
  "name": "@buttery/studio",
  "version": "0.3.3",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "react-router build && yarn server:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "react-router typegen && tsc",
    "typegen": "wrangler types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "server:build": "yarn tsc --project tsconfig.server.json",
    "serve": "yarn tsx ./scripts/serve.ts"
  },
  "files": [
    "build",
    "StudioServer.js",
    "StudioServer.d.ts"
  ],
  "exports": {
    "./server": "./StudioServer.js"
  },
  "dependencies": {
    "@buttery/studio-tokens": "0.3.3",
    "@linaria/server": "6.3.0",
    "@monaco-editor/react": "4.7.0",
    "@react-router/express": "7.6.0",
    "@react-router/fs-routes": "7.6.0",
    "@react-router/node": "7.6.0",
    "@react-router/serve": "7.6.0",
    "chroma-js": "^3.1.2",
    "express": "5.1.0",
    "immer": "10.1.1",
    "isbot": "5.1.28",
    "isoscribe": "0.2.0",
    "monaco-editor": "0.52.2",
    "puppeteer": "24.8.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-primitives": "0.2.3",
    "react-router": "7.6.0",
    "shiki": "3.4.2",
    "ts-jolt": "0.2.3",
    "ts-pattern": "5.7.1",
    "use-immer": "0.11.0",
    "vite-plugin-monaco-editor": "1.1.0",
    "zod": "3.25.4"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@chromatic-com/storybook": "3.2.6",
    "@gfdigital/tsconfig": "0.3.1",
    "@linaria/core": "6.3.0",
    "@react-router/dev": "7.6.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/babel__core": "7.20.5",
    "@types/chroma-js": "^3",
    "@types/express": "5.0.2",
    "@types/node": "22.15.19",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@wyw-in-js/vite": "0.6.0",
    "eslint": "9.27.0",
    "react-router-devtools": "5.0.5",
    "storybook": "8.6.14",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.15.2"
  }
}
