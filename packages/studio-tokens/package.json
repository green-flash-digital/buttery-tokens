{
  "name": "@buttery/studio-tokens",
  "version": "0.2.2",
  "description": "A standalone package to test the APIs in @buttery/core and to establish the tokens for the @buttery/studio",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js",
    "./root.css": "./.buttery/_generated/root.css"
  },
  "scripts": {
    "tokens:build": "yarn tsx ./scripts/build-studio-tokens.ts",
    "build": "yarn tokens:build && yarn tsc --project tsconfig.build.json",
    "dev": "yarn tsx ./scripts/dev-studio-tokens.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/green-flash-digital/buttery-tokens.git"
  },
  "author": "Drew DeCarme (drew@greenflash.digital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/green-flash-digital/buttery-tokens/issues"
  },
  "homepage": "https://github.com/green-flash-digital/buttery-tokens#readme",
  "devDependencies": {
    "@buttery/core": "0.2.2",
    "@gfdigital/tsconfig": "0.3.1",
    "isoscribe": "0.1.4",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  }
}
